{
  "publisher": "Ghosteye",
  "name": "vs-quick-log",
  "displayName": "Quick create log",
  "version": "0.1.3",
  "private": true,
  "description": "Identify and print variables quickly, accurately and efficiently.",
  "author": "_Ghosteye <Ghosteye@yeah.net>",
  "license": "MIT",
  "homepage": "https://github.com/xiaoyao-Ye/vs-quick-log#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoyao-Ye/vs-quick-log.git"
  },
  "bugs": {
    "url": "https://github.com/xiaoyao-Ye/vs-quick-log/issues"
  },
  "categories": [
    "Other",
    "Snippets",
    "Debuggers"
  ],
  "main": "./dist/index.js",
  "icon": "public/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.78.1"
  },
  "activationEvents": [
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "contributes": {
    "commands": [
      {
        "command": "log.createLog",
        "title": "Create Log"
      },
      {
        "command": "log.clearLog",
        "title": "Clear Log"
      }
    ],
    "keybindings": [
      {
        "command": "log.createLog",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l"
      },
      {
        "command": "log.clearLog",
        "key": "ctrl+shift+delete",
        "mac": "cmd+shift+delete"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "pnpm run build --watch",
    "vscode:prepublish": "pnpm run test && pnpm run build",
    "publish": "bumpp && vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish"
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@types/vscode": "^1.78.1",
    "@vscode/vsce": "^2.19.0",
    "bumpp": "^9.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.2.3",
    "vitest": "^0.31.4"
  }
}
